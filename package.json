{
	"name": "@cshr/lpg-admin",
	"version": "1.0.0",
	"description": "lpg admin application",
	"main": "server",
	"scripts": {
		"clean": "rm -rf dist",
		"sass": "node-sass views/assets/styles/main.scss dist/views/assets/styles/main.css --output-style compressed",
		"test": "npm run compile && find dist -name '*Test.js' | xargs ./node_modules/nyc/bin/nyc.js --reporter=text --reporter=html ./node_modules/mocha/bin/mocha",
		"compile": "./node_modules/typescript/bin/tsc",
		"build": "npm run sass && npm run compile",
		"watch-sass": "node-sass -w -r views/assets/styles/ -o dist/views/assets/styles/ --output-style compressed",
		"start": "npm run clean && npm run build && node dist/build/src/server.js",
		"cucumber": "./node_modules/cucumber/bin/cucumber-js test/acceptance/",
		"acceptance-test": "npm run start-mock-identity-service && npm run start-mock-learning-catalogue && npm run cucumber && npm run stop-mock-identity-service && npm run stop-mock-learning-catalogue",
		"start-mock-identity-service": "node ./test/acceptance/features/steps/lib/mock-server.js start -p 8080 -n identity_service",
		"stop-mock-identity-service": "node ./test/acceptance/features/steps/lib/mock-server.js stop -p 8080 -n identity_service",
		"start-mock-learning-catalogue": "node ./test/acceptance/features/steps/lib/mock-server.js start -p 9001 -n lerning_catalogue",
		"stop-mock-learning-catalogue": "node ./test/acceptance/features/steps/lib/mock-server.js stop -p 9001 -n learning_catalogue"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Civil-Service-Human-Resources/lpg-admin.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Civil-Service-Human-Resources/lpg-admin/issues"
	},
	"homepage": "https://github.com/Civil-Service-Human-Resources/lpg-admin#readme",
